{
  ":name": "DeGrowLab.com",
  ":description": "This is the CMS model for de-grow-lab. It is found in the directory of the 'target' branch in the git repository.",
  ":custom-types": {
    "breakpoint": {
      ":title": "Breakpoint",
      ":description": "Seperate a block into two components for mobile and desktop",
      ":type": "object",
      "mobile": {
        ":title": "Mobile",
        ":type": "typeOf",
        ":items": [
          "section",
          "paragraph",
          "heading",
          "chapter-heading",
          "large-image",
          "center-image",
          "link",
          "discover-flavours",
          "article",
          "products",
          "locations"
        ]
      },
      "desktop": {
        ":title": "Desktop",
        ":type": "typeOf",
        ":items": [
          "section",
          "paragraph",
          "heading",
          "chapter-heading",
          "large-image",
          "center-image",
          "link",
          "discover-flavours",
          "article",
          "products",
          "locations"
        ]
      }
    },
    "color": {
      ":title": "Color",
      ":description": "Type of color",
      ":placeholder": "softGreen, softBlue, softPink, softGrey, ...",
      ":type": "string"
    },
    "article": {
      ":title": "Article (Rich Text)",
      ":description": "Text that can contain formatting",
      ":type": "object",
      "body": {
        ":title": "Body",
        ":type": "rich-text"
      }
    },
    "paragraph": {
      ":title": "Paragraph (Rich Text)",
      ":description": "Text that can contain formatting",
      ":type": "object",
      "body": {
        ":title": "Body",
        ":type": "rich-text"
      }
    },
    "heading": {
      ":title": "Heading",
      ":description": "Basic heading component",
      ":type": "object",
      "title": {
        ":title": "Title",
        ":type": "string"
      },
      "varient": {
        ":title": "Varient",
        ":type": "string"
      }
    },
    "chapter-heading": {
      ":title": "Chapter Heading",
      ":description": "Chapter heading component",
      ":type": "object",
      "title": {
        ":title": "Title",
        ":type": "string"
      },
      "varient": {
        ":title": "Varient",
        ":type": "string"
      },
      "color": {
        ":title": "Color",
        ":type": "string",
        ":placeholder": "softGreen, softBlue, softPink ..."
      }
    },
    "link": {
      ":title": "Link (CTA)",
      ":description": "Link",
      ":type": "object",
      "text": {
        ":title": "Text",
        ":type": "string"
      },
      "href": {
        ":title": "Href (Link)",
        ":type": "string"
      }
    },
    "large-image": {
      ":title": "Large Image",
      ":type": "object",
      "marginTop": {
        ":title": "Top Margin",
        ":placeholder": "Example: -30px",
        ":type": "string"
      },
      "marginBottom": {
        ":title": "Bottom Margin",
        ":placeholder": "Example: -30px",
        ":type": "string"
      },
      "maxHeight": {
        ":title": "Max Height",
        ":placeholder": "Default: 650px",
        ":type": "string"
      },
      "picture": {
        ":title": "Picture",
        ":type": "picture"
      },
      "imageFit": {
        ":title": "Image Fit",
        ":placeholder": "contain or cover",
        ":type": "string"
      }
    },
    "center-image": {
      ":title": "Center Image",
      ":type": "object",
      "size": {
        ":title": "Size",
        ":placeholder": "small, large",
        ":type": "string"
      },
      "picture": {
        ":title": "Picture",
        ":type": "picture"
      }
    },
    "picture": {
      ":title": "Picture (uploadcare.com)",
      ":description": "Uploadcare image",
      ":type": "object",
      "alt": {
        ":title": "Alternative name",
        ":type": "string"
      },
      "image": {
        ":title": "Image",
        ":type": "image"
      }
    },
    "products": {
      ":title": "Products",
      ":type": "object",
      "products": {
        ":title": "Product List",
        ":type": "array",
        ":display-key": "title",
        ":items": {
          "picture": {
            ":title": "Picture",
            ":type": "picture"
          },
          "title": {
            ":title": "Title",
            ":type": "string"
          },
          "details": {
            ":title": "Details",
            ":type": "rich-text"
          },
          "whereToBuy": {
            ":title": "Where to buy",
            ":type": "string"
          },
          "price": {
            ":title": "Price",
            ":placeholder": "10, 25 ...",
            ":type": "string"
          }
        }
      }
    },
    "locations": {
      ":title": "Locations",
      ":type": "object",
      "locations": {
        ":title": "Locations",
        ":type": "array",
        ":display-key": "title",
        ":items": {
          "address": {
            ":title": "Address",
            ":type": "rich-text"
          },
          "link": {
            ":title": "Link",
            ":type": "string"
          }
        }
      }
    },
    "section": {
      ":title": "Section",
      ":type": "object",
      "id": {
        ":title": "ID",
        ":type": "string",
        ":placeholder": "example-id"
      },
      "color": {
        ":title": "Color",
        ":type": "string",
        ":placeholder": "softGreen, softBlue, softPink ..."
      },
      "gradient": {
        ":title": "Gradient",
        ":description": "Gradient type",
        ":type": "array",
        ":items": "color"
      },
      "items": {
        ":title": "Items",
        ":type": "arrayOf",
        ":items": [
          "section",
          "paragraph",
          "heading",
          "chapter-heading",
          "large-image",
          "center-image",
          "link",
          "discover-flavours",
          "breakpoint",
          "article",
          "products",
          "locations"
        ]
      }
    },
    "discover-flavours": {
      ":title": "Discover Flavours",
      ":type": "object"
    },
    "newsletter": {
      ":title": "Newsletter (Form)",
      ":type": "object"
    }
  },
  "navigation": {
    ":title": "Navigation",
    ":type": "object",
    "title": {
      ":title": "Title",
      ":type": "string",
      ":placeholer": "Title Text"
    },
    "homePath": {
      ":title": "Home path",
      ":type": "string",
      ":placeholer": "Home path"
    },
    "colors": {
      ":title": "Navigation colors",
      ":type": "array",
      ":display-key": "path",
      ":items": {
        "path": {
          ":title": "Path",
          ":type": "string",
          ":placeholder": "/path/location"
        },
        "color": {
          ":title": "Color",
          ":type": "string",
          ":placeholder": "softGreen, softBlue, softPink ..."
        }
      }
    },
    "options": {
      ":title": "Navigation Options",
      ":type": "array",
      ":display-key": "label",
      ":items": {
        "label": {
          ":title": "Label",
          ":type": "string",
          ":placeholder": "Text label"
        },
        "path": {
          ":title": "Path",
          ":type": "string",
          ":placeholder": "/path/location"
        },
        "target": {
          ":title": "Target (optional)",
          ":type": "string",
          ":placeholder": "eg. blank"
        },
        "items": {
          ":title": "Items (Children)",
          ":type": "array",
          ":display-key": "label",
          ":items": {
            "label": {
              ":title": "Label",
              ":type": "string",
              ":placeholder": "Text label"
            },
            "path": {
              ":title": "Path",
              ":type": "string",
              ":placeholder": "/path/location"
            },
            "target": {
              ":title": "Target (optional)",
              ":type": "string",
              ":placeholder": "eg. blank"
            }
          }
        }
      }
    }
  },
  "pages": {
    ":title": "Pages",
    ":description": "A data object for each page",
    ":type": "array",
    ":display-key": "slug",
    ":items": {
      "slug": {
        ":title": "Slug",
        ":type": "string",
        ":placeholder": "id-for-page"
      },
      "seo": {
        ":title": "SEO",
        ":type": "object",
        "title": {
          ":type": "string"
        },
        "description": {
          ":type": "string"
        }
      },
      "blocks": {
        ":title": "Blocks",
        ":type": "arrayOf",
        ":display-key": "slug",
        ":items": [
          "section",
          "paragraph",
          "heading",
          "chapter-heading",
          "large-image",
          "center-image",
          "link",
          "discover-flavours",
          "breakpoint",
          "article",
          "products",
          "locations"
        ]
      }
    }
  },
  "defaults": {
    ":title": "Defaults",
    ":description": "Default values",
    ":type": "object",
    "seo": {
      ":title": "SEO",
      ":type": "object",
      "title": {
        ":type": "string"
      },
      "description": {
        ":type": "string"
      }
    }
  },
  "flavours": {
    ":title": "Flavours",
    ":description": "List of all flavours",
    ":type": "array",
    ":display-key": "slug",
    ":items": {
      "slug": {
        ":title": "Slug",
        ":type": "string",
        ":placeholder": "flavour-slug"
      },
      "title": {
        ":title": "Title",
        ":type": "string"
      },
      "gradient": {
        ":title": "Gradient",
        ":description": "Gradient type",
        ":type": "array",
        ":items": "color"
      },
      "color": {
        ":title": "Color",
        ":type": "string",
        ":placeholder": "softGreen, softBlue, softPink ..."
      },
      "isCollaboration": {
        ":title": "Is Collaboration",
        ":type": "string",
        ":placeholder": "0 or 1"
      },
      "detailPage": {
        ":title": "Detail Page",
        ":type": "object",
        "description": {
          ":title": "Description",
          ":type": "string",
          ":placeholder": "Description"
        },
        "ingredients": {
          ":title": "Ingredients",
          ":type": "string",
          ":placeholder": "Ingredients"
        },
        "nutrition": {
          ":title": "Nutrition",
          ":type": "rich-text",
          ":placeholder": "Nutrition"
        }
      },
      "discover": {
        ":title": "Discover layout",
        ":type": "object",
        "yOffset": {
          ":title": "yOffset",
          ":type": "string",
          ":placeholder": "-30%"
        },
        "rotate": {
          ":title": "Rotate",
          ":type": "string",
          ":placeholder": "30"
        },
        "stiffness": {
          ":title": "Stiffnes",
          ":type": "string",
          ":placeholder": "0.9"
        }
      },
      "picture": {
        ":title": "Picture",
        ":type": "picture"
      }
    }
  },
  "footer": {
    ":title": "Footer",
    ":description": "Footer for website",
    ":type": "object",
    "title": {
      ":title": "Title",
      ":type": "string",
      ":placeholder": "Title for footer"
    }
  }
}
